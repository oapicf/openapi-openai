{
  "namespace": "model",
  "type": "record",
  "doc": "Send this event to truncate a previous assistant messageâ€™s audio. The server  will produce audio faster than realtime, so this event is useful when the user  interrupts to truncate audio that has already been sent to the client but not  yet played. This will synchronize the server's understanding of the audio with  the client's playback.  Truncating audio will delete the server-side text transcript to ensure there  is not text in the context that hasn't been heard by the user.  If successful, the server will respond with a `conversation.item.truncated`  event.  ",
  "name": "RealtimeClientEventConversationItemTruncate",
  "fields": [
    {
      "name": "event_id",
      "type": ["null", "string"],
      "doc": "Optional client-generated ID used to identify this event.",
      "default": null
    },
    {
      "name": "type",
      "type": {
        "type": "enum",
        "name": "RealtimeClientEventConversationItemTruncate_type",
        "symbols": [
          "conversation_item_truncate"
        ]
      },
      "doc": "The event type, must be `conversation.item.truncate`."
    },
    {
      "name": "item_id",
      "type": "string",
      "doc": "The ID of the assistant message item to truncate. Only assistant message  items can be truncated. "
    },
    {
      "name": "content_index",
      "type": "int",
      "doc": "The index of the content part to truncate. Set this to 0."
    },
    {
      "name": "audio_end_ms",
      "type": "int",
      "doc": "Inclusive duration up to which audio is truncated, in milliseconds. If  the audio_end_ms is greater than the actual audio duration, the server  will respond with an error. "
    }
  ]

}
