{
  "namespace": "model",
  "type": "record",
  "doc": "The item to add to the conversation.",
  "name": "RealtimeConversationItem",
  "fields": [
    {
      "name": "id",
      "type": ["null", "string"],
      "doc": "The unique ID of the item, this can be generated by the client to help  manage server-side context, but is not required because the server will  generate one if not provided. ",
      "default": null
    },
    {
      "name": "type",
      "type": ["null", {
        "type": "enum",
        "name": "RealtimeConversationItem_type",
        "symbols": [
          "message",
          "function_call",
          "function_call_output"
        ]
      }],
      "doc": "The type of the item (`message`, `function_call`, `function_call_output`). ",
      "default": null
    },
    {
      "name": "object",
      "type": ["null", {
        "type": "enum",
        "name": "RealtimeConversationItem_object",
        "symbols": [
          "realtime_item"
        ]
      }],
      "doc": "Identifier for the API object being returned - always `realtime.item`. ",
      "default": null
    },
    {
      "name": "status",
      "type": ["null", {
        "type": "enum",
        "name": "RealtimeConversationItem_status",
        "symbols": [
          "completed",
          "incomplete"
        ]
      }],
      "doc": "The status of the item (`completed`, `incomplete`). These have no effect  on the conversation, but are accepted for consistency with the  `conversation.item.created` event. ",
      "default": null
    },
    {
      "name": "role",
      "type": ["null", {
        "type": "enum",
        "name": "RealtimeConversationItem_role",
        "symbols": [
          "user",
          "assistant",
          "system"
        ]
      }],
      "doc": "The role of the message sender (`user`, `assistant`, `system`), only  applicable for `message` items. ",
      "default": null
    },
    {
      "name": "content",
      "type": ["null", {
        "type": "array",
        "items": "model.RealtimeConversationItemContentInner"
      }],
      "doc": "The content of the message, applicable for `message` items.  - Message items of role `system` support only `input_text` content - Message items of role `user` support `input_text` and `input_audio`    content - Message items of role `assistant` support `text` content. ",
      "default": null
    },
    {
      "name": "call_id",
      "type": ["null", "string"],
      "doc": "The ID of the function call (for `function_call` and  `function_call_output` items). If passed on a `function_call_output`  item, the server will check that a `function_call` item with the same  ID exists in the conversation history. ",
      "default": null
    },
    {
      "name": "name",
      "type": ["null", "string"],
      "doc": "The name of the function being called (for `function_call` items). ",
      "default": null
    },
    {
      "name": "arguments",
      "type": ["null", "string"],
      "doc": "The arguments of the function call (for `function_call` items). ",
      "default": null
    },
    {
      "name": "output",
      "type": ["null", "string"],
      "doc": "The output of the function call (for `function_call_output` items). ",
      "default": null
    }
  ]

}
