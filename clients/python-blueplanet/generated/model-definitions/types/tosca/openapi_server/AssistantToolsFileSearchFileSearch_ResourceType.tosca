"$schema"   = "http://cyaninc.com/json-schemas/tosca-lite-v1/definition-module#"
title       = "openapi_server AssistantToolsFileSearchFileSearch"
package     = openapi_server
version     = "1.0"
description = "Defines a AssistantToolsFileSearchFileSearch"
authors     = ["F. Bar (foo@bar.baz)"]

imports {
  Root = tosca.resourceTypes.Root
  FileSearchRankingOptions = openapi_server.resourceTypes.FileSearchRankingOptions
}
resourceTypes {

  AssistantToolsFileSearchFileSearch {
    title = AssistantToolsFileSearchFileSearch
    description = AssistantToolsFileSearchFileSearch
    derivedFrom = Root
    properties {
        max_num_results {
          type =  integer
          description = "The maximum number of results the file search tool should output. The default is 20 for &#x60;gpt-4*&#x60; models and 5 for &#x60;gpt-3.5-turbo&#x60;. This number should be between 1 and 50 inclusive.  Note that the file search tool may output fewer than &#x60;max_num_results&#x60; results. See the [file search tool documentation](/docs/assistants/tools/file-search#customizing-file-search-settings) for more information. "
          optional = false
        }
        ranking_options {
          type = FileSearchRankingOptions
          description = ""
          optional = false
        }
    }
  }
}

serviceTemplates {

  AssistantToolsFileSearchFileSearch {
    title = AssistantToolsFileSearchFileSearch
    description = AssistantToolsFileSearchFileSearch
    implements = openapi_server.resourceTypes.AssistantToolsFileSearchFileSearch

  }
}
