"$schema"   = "http://cyaninc.com/json-schemas/tosca-lite-v1/definition-module#"
title       = "openapi_server BatchRequestInput"
package     = openapi_server
version     = "1.0"
description = "Defines a BatchRequestInput"
authors     = ["F. Bar (foo@bar.baz)"]

imports {
  Root = tosca.resourceTypes.Root
}
resourceTypes {

  BatchRequestInput {
    title = BatchRequestInput
    description = BatchRequestInput
    derivedFrom = Root
    properties {
        custom_id {
          type =  string
          description = "A developer-provided per-request id that will be used to match outputs to inputs. Must be unique for each request in a batch."
          optional = false
        }
        method {
          type =  string
          description = "The HTTP method to be used for the request. Currently only &#x60;POST&#x60; is supported."
          enum = ["POST"]
          optional = false
        }
        url {
          type =  string
          description = "The OpenAI API relative URL to be used for the request. Currently &#x60;/v1/chat/completions&#x60;, &#x60;/v1/embeddings&#x60;, and &#x60;/v1/completions&#x60; are supported."
          optional = false
        }
    }
  }
}

serviceTemplates {

  BatchRequestInput {
    title = BatchRequestInput
    description = BatchRequestInput
    implements = openapi_server.resourceTypes.BatchRequestInput

  }
}
