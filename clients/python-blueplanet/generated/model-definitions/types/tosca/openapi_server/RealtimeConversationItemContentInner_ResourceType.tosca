"$schema"   = "http://cyaninc.com/json-schemas/tosca-lite-v1/definition-module#"
title       = "openapi_server RealtimeConversationItemContentInner"
package     = openapi_server
version     = "1.0"
description = "Defines a RealtimeConversationItemContentInner"
authors     = ["F. Bar (foo@bar.baz)"]

imports {
  Root = tosca.resourceTypes.Root
}
resourceTypes {

  RealtimeConversationItemContentInner {
    title = RealtimeConversationItemContentInner
    description = RealtimeConversationItemContentInner
    derivedFrom = Root
    properties {
        type {
          type =  string
          description = "The content type (&#x60;input_text&#x60;, &#x60;input_audio&#x60;, &#x60;item_reference&#x60;, &#x60;text&#x60;). "
          enum = ["input_audio", "input_text", "item_reference", "text"]
          optional = false
        }
        text {
          type =  string
          description = "The text content, used for &#x60;input_text&#x60; and &#x60;text&#x60; content types. "
          optional = false
        }
        id {
          type =  string
          description = "ID of a previous conversation item to reference (for &#x60;item_reference&#x60; content types in &#x60;response.create&#x60; events). These can reference both client and server created items. "
          optional = false
        }
        audio {
          type =  string
          description = "Base64-encoded audio bytes, used for &#x60;input_audio&#x60; content type. "
          optional = false
        }
        transcript {
          type =  string
          description = "The transcript of the audio, used for &#x60;input_audio&#x60; content type. "
          optional = false
        }
    }
  }
}

serviceTemplates {

