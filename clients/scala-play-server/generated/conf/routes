# Routes for Projects API

POST /v1/organization/projects                                                    api.ProjectsApiController.createProject()
GET /v1/organization/projects                                                     api.ProjectsApiController.listProjects()
POST /v1/organization/projects/:projectId/archive                                 api.ProjectsApiController.archiveProject(projectId: String)
POST /v1/organization/projects/:projectId/service_accounts                        api.ProjectsApiController.createProjectServiceAccount(projectId: String)
POST /v1/organization/projects/:projectId/users                                   api.ProjectsApiController.createProjectUser(projectId: String)
DELETE /v1/organization/projects/:projectId/api_keys/:keyId                       api.ProjectsApiController.deleteProjectApiKey(projectId: String, keyId: String)
DELETE /v1/organization/projects/:projectId/service_accounts/:serviceAccountId    api.ProjectsApiController.deleteProjectServiceAccount(projectId: String, serviceAccountId: String)
DELETE /v1/organization/projects/:projectId/users/:userId                         api.ProjectsApiController.deleteProjectUser(projectId: String, userId: String)
GET /v1/organization/projects/:projectId/api_keys                                 api.ProjectsApiController.listProjectApiKeys(projectId: String)
GET /v1/organization/projects/:projectId/rate_limits                              api.ProjectsApiController.listProjectRateLimits(projectId: String)
GET /v1/organization/projects/:projectId/service_accounts                         api.ProjectsApiController.listProjectServiceAccounts(projectId: String)
GET /v1/organization/projects/:projectId/users                                    api.ProjectsApiController.listProjectUsers(projectId: String)
POST /v1/organization/projects/:projectId                                         api.ProjectsApiController.modifyProject(projectId: String)
POST /v1/organization/projects/:projectId/users/:userId                           api.ProjectsApiController.modifyProjectUser(projectId: String, userId: String)
GET /v1/organization/projects/:projectId                                          api.ProjectsApiController.retrieveProject(projectId: String)
GET /v1/organization/projects/:projectId/api_keys/:keyId                          api.ProjectsApiController.retrieveProjectApiKey(projectId: String, keyId: String)
GET /v1/organization/projects/:projectId/service_accounts/:serviceAccountId       api.ProjectsApiController.retrieveProjectServiceAccount(projectId: String, serviceAccountId: String)
GET /v1/organization/projects/:projectId/users/:userId                            api.ProjectsApiController.retrieveProjectUser(projectId: String, userId: String)
POST /v1/organization/projects/:projectId/rate_limits/:rateLimitId                api.ProjectsApiController.updateProjectRateLimits(projectId: String, rateLimitId: String)

