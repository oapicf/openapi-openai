# Routes for VectorStores API

POST /v1/vector_stores                                                            api.VectorStoresApiController.createVectorStore()
GET /v1/vector_stores                                                             api.VectorStoresApiController.listVectorStores()
POST /v1/vector_stores/:vectorStoreId/file_batches/:batchId/cancel                api.VectorStoresApiController.cancelVectorStoreFileBatch(vectorStoreId: String, batchId: String)
POST /v1/vector_stores/:vectorStoreId/files                                       api.VectorStoresApiController.createVectorStoreFile(vectorStoreId: String)
POST /v1/vector_stores/:vectorStoreId/file_batches                                api.VectorStoresApiController.createVectorStoreFileBatch(vectorStoreId: String)
DELETE /v1/vector_stores/:vectorStoreId                                           api.VectorStoresApiController.deleteVectorStore(vectorStoreId: String)
DELETE /v1/vector_stores/:vectorStoreId/files/:fileId                             api.VectorStoresApiController.deleteVectorStoreFile(vectorStoreId: String, fileId: String)
GET /v1/vector_stores/:vectorStoreId                                              api.VectorStoresApiController.getVectorStore(vectorStoreId: String)
GET /v1/vector_stores/:vectorStoreId/files/:fileId                                api.VectorStoresApiController.getVectorStoreFile(vectorStoreId: String, fileId: String)
GET /v1/vector_stores/:vectorStoreId/file_batches/:batchId                        api.VectorStoresApiController.getVectorStoreFileBatch(vectorStoreId: String, batchId: String)
GET /v1/vector_stores/:vectorStoreId/file_batches/:batchId/files                  api.VectorStoresApiController.listFilesInVectorStoreBatch(vectorStoreId: String, batchId: String)
GET /v1/vector_stores/:vectorStoreId/files                                        api.VectorStoresApiController.listVectorStoreFiles(vectorStoreId: String)
POST /v1/vector_stores/:vectorStoreId                                             api.VectorStoresApiController.modifyVectorStore(vectorStoreId: String)

# Map static resources from the /public folder to the /assets URL path
GET /assets/*file           controllers.Assets.at(file)
GET /versionedAssets/*file  controllers.Assets.versioned(file)

# Swagger UI
GET /api                    api.ApiDocController.api
